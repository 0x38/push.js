{
  "name": "push.js",
  "version": "1.0.5",
  "description": "A compact, cross-browser solution for the Javascript Notifications API",
  "main": "bin/push.min.js",
  "scripts": {
    "clean": "rimraf bin/",
    "tc": "tsc --noEmit",
    "build": "tsc && rollup -c && uglifyjs --source-map -o bin/serviceWorker.min.js src/serviceWorker.js",
    "test": "npm run build && karma start tests/karma.conf.js",
    "prepublish": "npm run build",
    "precommit": "lint-staged && npm run build && git add ./bin"
  },
  "files": [
    "bin",
    "*.md",
    "*.png"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Nickersoft/push.js"
  },
  "author": "Tyler Nickerson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nickersoft/push.js/issues"
  },
  "homepage": "https://github.com/Nickersoft/push.js",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.51",
    "@babel/plugin-transform-strict-mode": "^7.0.0-beta.51",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-stage-2": "^7.0.0-beta.51",
    "browserify": "^16.2.2",
    "coveralls": "^3.0.1",
    "husky": "^0.14.3",
    "jasmine-core": "^3.1.0",
    "js-yaml": "^3.12.0",
    "karma": "^2.0.3",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "lint-staged": "^7.2.0",
    "platform": "^1.3.5",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.7",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "uglify-es": "^3.3.9"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.0.0-beta.51",
    "typescript": "^2.9.2"
  }
}
